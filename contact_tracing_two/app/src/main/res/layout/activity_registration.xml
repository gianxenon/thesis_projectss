<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ecf0f3"

    tools:context=".registration">


    <soup.neumorphism.NeumorphCardView
        android:id="@+id/neumorphCardView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.133"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/log_imgLogo2"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_centerVertical="true"
                android:layout_margin="15dp"
                android:contentDescription="@string/log_imgLogo"
                app:srcCompat="@drawable/logo_two" />

            <TextView
                android:id="@+id/reg_txtV_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginBottom="15dp"
                android:layout_toEndOf="@id/log_imgLogo2"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:text="@string/reg_txtV_title"
                android:textSize="26sp"
                android:textStyle="bold"
                android:textColor="@color/black"/>
        </RelativeLayout>


    </soup.neumorphism.NeumorphCardView>

    <TextView
        android:id="@+id/reg_txtV_regTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="@string/reg_txtV_regTitle"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/neumorphCardView3" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"

        android:layout_height="match_parent"
        android:layout_marginTop="236dp"
        android:paddingTop="20dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="-29dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="10dp"
            android:paddingBottom="50dp">


            <TextView
                android:id="@+id/reg_txtV_Fname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="@string/reg_txtV_Fname" />

            <soup.neumorphism.NeumorphCardView
                android:id="@+id/neumorphCardView4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#efefef"
                app:neumorph_shadowColorDark="#d9d9d9"
                app:neumorph_shadowColorLight="#fff"
                app:neumorph_shadowElevation="5dp"
                app:neumorph_shapeType="pressed"
                tools:layout_editor_absoluteY="244dp">

                <EditText
                    android:id="@+id/reg_edt_Fname"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="10dp"
                    android:padding="5dp"
                    android:background="@drawable/no_background"
                    android:ems="10"
                    android:textColorHint="#808080"
                    android:hint="@string/reg_edt_Fname"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"

                    tools:layout_editor_absoluteX="22dp"
                    tools:layout_editor_absoluteY="202dp" />
            </soup.neumorphism.NeumorphCardView>

            <TextView
                android:id="@+id/reg_txtV_Mname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="@string/reg_txtV_Mname" />

            <soup.neumorphism.NeumorphCardView
                android:id="@+id/neumorphCardView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#efefef"
                app:neumorph_shadowColorDark="#d9d9d9"
                app:neumorph_shadowColorLight="#fff"
                app:neumorph_shadowElevation="5dp"
                app:neumorph_shapeType="pressed"
                tools:layout_editor_absoluteY="353dp">

                <EditText
                    android:id="@+id/reg_edt_Mname"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="10dp"
                    android:background="@drawable/no_background"
                    android:ems="10"
                    android:textColorHint="#808080"
                    android:hint="@string/reg_edt_Mname"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:padding="5dp" />
            </soup.neumorphism.NeumorphCardView>

            <TextView
                android:id="@+id/reg_txtV_Lname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginStart="15dp"
                android:text="@string/reg_txtV_Lname" />


            <soup.neumorphism.NeumorphCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#efefef"
                app:neumorph_shadowColorDark="#d9d9d9"
                app:neumorph_shadowColorLight="#fff"
                app:neumorph_shadowElevation="5dp"
                app:neumorph_shapeType="pressed"
                tools:layout_editor_absoluteY="450dp">

                <EditText
                    android:id="@+id/reg_edt_Lname"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="10dp"
                    android:background="@drawable/no_background"
                    android:ems="10"
                    android:hint="@string/reg_edt_Lname"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:padding="5dp"
                    android:textColorHint="#808080" />
            </soup.neumorphism.NeumorphCardView>
            <TextView

                android:id="@+id/reg_txtV_Email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="@string/reg_txtV_Email" />

            <soup.neumorphism.NeumorphCardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:neumorph_shadowColorDark="#d9d9d9"
                app:neumorph_shadowColorLight="#fff"
                app:neumorph_shadowElevation="5dp"
                app:neumorph_shapeType="pressed"
                >


                <EditText
                    android:id="@+id/reg_edt_email"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/no_background"
                    android:ems="10"
                    android:hint="@string/reg_edt_email"
                    android:inputType="textEmailAddress"
                    android:layout_margin="10dp"
                    android:textColorHint="#808080"
                    android:padding="5dp"
                    android:importantForAutofill="no" />
            </soup.neumorphism.NeumorphCardView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="@string/reg_txtV_ContactNum" />

            <soup.neumorphism.NeumorphCardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:neumorph_shadowColorDark="#d9d9d9"
                app:neumorph_shadowColorLight="#fff"
                app:neumorph_shadowElevation="5dp"
                app:neumorph_shapeType="pressed"
                >


                <EditText
                    android:id="@+id/reg_edt_ContactNum"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="10dp"
                    android:background="@drawable/no_background"
                    android:ems="10"
                    android:hint="@string/reg_edt_ContactNum"
                    android:importantForAutofill="no"
                    android:inputType="textEmailAddress"
                    android:padding="5dp"
                    android:textColorHint="#808080" />
            </soup.neumorphism.NeumorphCardView>
            <TextView
                android:id="@+id/reg_txtV_Bdate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginStart="15dp"
                android:text="@string/reg_txtV_Bdate" />


            <soup.neumorphism.NeumorphCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#efefef"
                app:neumorph_shadowColorDark="#d9d9d9"
                app:neumorph_shadowColorLight="#fff"
                app:neumorph_shadowElevation="5dp"
                app:neumorph_shapeType="pressed"
                tools:layout_editor_absoluteY="450dp">

                <EditText
                    android:id="@+id/reg_edt_Bdate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:background="@drawable/no_background"
                    android:drawableEnd="@drawable/ic_baseline_calendar_today_24"
                    android:ems="10"
                    android:textColorHint="#808080"
                    android:hint="@string/reg_edt_Bdate"
                    android:focusable="false"
                    android:importantForAutofill="no"
                    android:inputType="none"
                    android:padding="5dp" />
            </soup.neumorphism.NeumorphCardView>


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/username"
                android:layout_marginStart="15dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                />
            <soup.neumorphism.NeumorphCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#efefef"
                app:neumorph_shadowColorDark="#d9d9d9"
                app:neumorph_shadowColorLight="#fff"
                app:neumorph_shadowElevation="5dp"
                app:neumorph_shapeType="pressed"
                tools:layout_editor_absoluteY="450dp">

                <EditText
                    android:id="@+id/reg_edt_usr"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:layout_margin="10dp"
                    android:textColorHint="#808080"
                    android:padding="5dp"
                    android:background="@drawable/no_background"
                    android:importantForAutofill="no"
                    android:hint="@string/usernamess"
                    tools:ignore="LabelFor" />
            </soup.neumorphism.NeumorphCardView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/sadwasdadaaa"
                android:layout_marginStart="15dp"
                android:textColor="@color/black"
                android:textStyle="bold"

                />
            <soup.neumorphism.NeumorphCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#efefef"
                app:neumorph_shadowColorDark="#d9d9d9"
                app:neumorph_shadowColorLight="#fff"
                app:neumorph_shadowElevation="5dp"
                app:neumorph_shapeType="pressed"
                android:padding="10dp"

                >

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/reg_txtIn_pass"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:boxBackgroundMode="none"
                    app:boxStrokeWidth="0dp"
                    app:hintEnabled="false"
                    app:passwordToggleEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:hint="@string/sssss"
                        android:padding="5dp"
                        android:singleLine="true"
                        android:textSize="18sp" />


                </com.google.android.material.textfield.TextInputLayout>

            </soup.neumorphism.NeumorphCardView>
            <TextView
                android:id="@+id/reg_txtV_Gender"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="@string/reg_txtV_Gender" />

            <soup.neumorphism.NeumorphCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#efefef"
                app:neumorph_shadowColorDark="#d9d9d9"
                app:neumorph_shadowColorLight="#fff"
                app:neumorph_shadowElevation="5dp"
                app:neumorph_shapeType="pressed"
                tools:layout_editor_absoluteY="450dp">


                <Spinner
                    android:id="@+id/reg_edt_gender"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:padding="10dp"
                    android:nextFocusDown="@id/reg_spnr_Region"
                    android:drawableEnd="@drawable/ic_baseline_keyboard_arrow_down_24"

                    />
            </soup.neumorphism.NeumorphCardView>
            <TextView
                android:id="@+id/reg_txtV_Region"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="@string/reg_txtV_Region" />
            <soup.neumorphism.NeumorphCardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:neumorph_shadowColorDark="#d9d9d9"
                app:neumorph_shadowColorLight="#fff"
                app:neumorph_shadowElevation="5dp"
                app:neumorph_shapeType="pressed"
                tools:layout_editor_absoluteY="450dp"
                >

                <Spinner
                    android:id="@+id/reg_spnr_Region"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:nextFocusDown="@+id/reg_spnr_Province"
                    android:drawableEnd="@drawable/ic_baseline_keyboard_arrow_down_24"
                    android:padding="10dp" />
            </soup.neumorphism.NeumorphCardView>

            <TextView

                android:id="@+id/reg_txtV_Province"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="@string/reg_txtV_Province" />

            <soup.neumorphism.NeumorphCardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:neumorph_shadowColorDark="#d9d9d9"
                app:neumorph_shadowColorLight="#fff"
                app:neumorph_shadowElevation="5dp"
                app:neumorph_shapeType="pressed"
                >
                <Spinner
                    android:id="@+id/reg_spnr_Province"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"

                    android:nextFocusDown="@+id/reg_spnr_Municipality"
                    android:drawableEnd="@drawable/ic_baseline_keyboard_arrow_down_24"
                    android:padding="10dp" />
            </soup.neumorphism.NeumorphCardView>

            <TextView
                android:id="@+id/reg_txtV_Municipality"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="@string/reg_txtV_Municipality" />

            <soup.neumorphism.NeumorphCardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:neumorph_shadowColorDark="#d9d9d9"
                app:neumorph_shadowColorLight="#fff"
                app:neumorph_shadowElevation="5dp"
                app:neumorph_shapeType="pressed"
                >

                <Spinner
                    android:id="@+id/reg_spnr_Municipality"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:nextFocusDown="@+id/reg_spnr_brgy"
                    android:drawableEnd="@drawable/ic_baseline_keyboard_arrow_down_24"
                    android:padding="10dp" />
            </soup.neumorphism.NeumorphCardView>

            <TextView
                android:id="@+id/reg_txtV_Brgy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="@string/reg_txtV_Brgy" />

            <soup.neumorphism.NeumorphCardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:neumorph_shadowColorDark="#d9d9d9"
                app:neumorph_shadowColorLight="#fff"
                app:neumorph_shadowElevation="5dp"
                app:neumorph_shapeType="pressed"
                >

                <Spinner
                    android:id="@+id/reg_spnr_brgy"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"

                    android:drawableEnd="@drawable/ic_baseline_keyboard_arrow_down_24"
                    android:padding="10dp" />
            </soup.neumorphism.NeumorphCardView>




            <Button
               android:id="@+id/reg_Btn_SignUp"
               android:layout_gravity="center"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:background="@drawable/button_corner"
                android:padding="10dp"
                android:textSize="16sp"
                android:textStyle="bold"
               android:text="@string/reg_Btn_SignUp"/>

        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>